{
  "mcpServers": {
    "power-figma-figma": {
      "url": "https://mcp.figma.com/mcp",
      "disabled": false,
      "disabledTools": []
    },
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "PATH": "/Users/jerry/.nvm/versions/node/v22.18.0/bin:/usr/local/bin:/usr/bin:/bin"
      },
      "disabled": false,
      "autoApprove": []
    },
    "prompt-kit": {
      "description": "prompt-kit registry",
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "shadcn@canary", "mcp"],
      "env": {
        "REGISTRY_URL": "https://www.prompt-kit.com/c/registry.json",
        "FASTMCP_LOG_LEVEL": "ERROR",
        "PATH": "/Users/jerry/.nvm/versions/node/v22.18.0/bin:/usr/local/bin:/usr/bin:/bin"
      },
      "disabled": false,
      "autoApprove": []
    },
    "augment-context-engine": {
      "command": "auggie",
      "type": "stdio",
      "args": ["--mcp", "--mcp-auto-workspace"],
      "disabled": false,
      "alwaysAllow": ["codebase-retrieval"]
    }
  }
}
